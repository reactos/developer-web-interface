{
	"name": "dev-interface-ros",
	"version": "1.0.0",
	"description": "developer web interface for ReactOS",
	"main": "app.js",
	"scripts": {
		"client-install": "npm install --prefix client",
		"start": "node app.js",
		"server": "nodemon app.js",
